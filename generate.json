{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "properties": {
        "annotations": {
            "properties": {
                "ingress": {
                    "properties": {
                        "nginx.ingress.kubernetes.io/proxy-body-size": {
                            "type": "string"
                        },
                        "nginx.ingress.kubernetes.io/use-regex": {
                            "type": "string"
                        },
                        "nginx.org/client-max-body-size": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "preupgradejob": {
                    "properties": {
                        "helm.sh/hook": {
                            "type": "string"
                        },
                        "helm.sh/hook-delete-policy": {
                            "type": "string"
                        },
                        "helm.sh/hook-weight": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "vault": {
                    "properties": {
                        "helm.sh/hook": {
                            "type": "string"
                        },
                        "helm.sh/hook-weight": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                }
            },
            "type": "object"
        },
        "autoscaling": {
            "properties": {
                "averageUtilCpu": {
                    "type": "integer"
                },
                "averageUtilMem": {
                    "type": "integer"
                },
                "enabled": {
                    "type": "boolean"
                },
                "maxReplicas": {
                    "type": "integer"
                },
                "minReplicas": {
                    "type": "integer"
                }
            },
            "type": "object"
        },
        "fullnameOverride": {
            "type": "string"
        },
        "gracePeriodSeconds": {
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "value": {
                    "type": "integer"
                }
            },
            "type": "object"
        },
        "hostAliases": {
            "items": {
                "properties": {
                    "hostnames": {
                        "items": {
                            "type": "string"
                        },
                        "type": "array"
                    },
                    "ip": {
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "type": "array"
        },
        "image": {
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "imagePullSecrets": {
            "items": {
                "properties": {
                    "name": {
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "type": "array"
        },
        "ingress": {
            "properties": {
                "configs": {
                    "items": {
                        "properties": {
                            "className": {
                                "type": "string"
                            },
                            "ingressName": {
                                "type": "string"
                            },
                            "rules": {
                                "items": {
                                    "properties": {
                                        "host": {
                                            "type": "string"
                                        },
                                        "http": {
                                            "properties": {
                                                "paths": {
                                                    "items": {
                                                        "properties": {
                                                            "backend": {
                                                                "properties": {
                                                                    "service": {
                                                                        "properties": {
                                                                            "name": {
                                                                                "type": "string"
                                                                            },
                                                                            "port": {
                                                                                "properties": {
                                                                                    "number": {
                                                                                        "type": "integer"
                                                                                    }
                                                                                },
                                                                                "type": "object"
                                                                            }
                                                                        },
                                                                        "type": "object"
                                                                    }
                                                                },
                                                                "type": "object"
                                                            },
                                                            "path": {
                                                                "type": "string"
                                                            },
                                                            "pathType": {
                                                                "type": "string"
                                                            }
                                                        },
                                                        "type": "object"
                                                    },
                                                    "type": "array"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                },
                                "type": "array"
                            }
                        },
                        "type": "object"
                    },
                    "type": "array"
                },
                "enabled": {
                    "type": "boolean"
                }
            },
            "type": "object"
        },
        "interceptor": {
            "properties": {
                "args": {
                    "type": "string"
                },
                "enabled": {
                    "type": "boolean"
                }
            },
            "type": "object"
        },
        "liveness": {
            "properties": {
                "failureThreshold": {
                    "type": "integer"
                },
                "initialDelay": {
                    "type": "integer"
                },
                "path": {
                    "type": "string"
                },
                "periodSeconds": {
                    "type": "integer"
                },
                "port": {
                    "type": "integer"
                },
                "successThreshold": {
                    "type": "integer"
                },
                "type": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "migrations": {
            "properties": {
                "args": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "enabled": {
                    "type": "boolean"
                },
                "type": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "nameOverride": {
            "type": "string"
        },
        "namespace": {
            "type": "string"
        },
        "replicaCount": {
            "type": "integer"
        },
        "resources": {
            "properties": {
                "limits": {
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "requests": {
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                }
            },
            "type": "object"
        },
        "secrets": {
            "properties": {
                "configs": {
                    "properties": {
                        "autorestart": {
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                }
                            },
                            "type": "object"
                        },
                        "enabled": {
                            "type": "boolean"
                        },
                        "path": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                },
                "creds": {
                    "properties": {
                        "autorestart": {
                            "properties": {
                                "enabled": {
                                    "type": "boolean"
                                }
                            },
                            "type": "object"
                        },
                        "enabled": {
                            "type": "boolean"
                        },
                        "path": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                }
            },
            "type": "object"
        },
        "service": {
            "properties": {
                "configs": {
                    "items": {
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "port": {
                                "type": "integer"
                            },
                            "protocol": {
                                "type": "string"
                            },
                            "targetPort": {
                                "type": "integer"
                            }
                        },
                        "type": "object"
                    },
                    "type": "array"
                },
                "headless": {
                    "type": "boolean"
                },
                "type": {
                    "type": "string"
                }
            },
            "type": "object"
        },
        "serviceAccount": {
            "properties": {
                "create": {
                    "type": "boolean"
                }
            },
            "type": "object"
        },
        "volumeMounts": {
            "items": {
                "properties": {
                    "mountPath": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "type": "array"
        },
        "volumes": {
            "items": {
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "secret": {
                        "properties": {
                            "secretName": {
                                "type": "string"
                            }
                        },
                        "type": "object"
                    }
                },
                "type": "object"
            },
            "type": "array"
        }
    },
    "type": "object"
}
